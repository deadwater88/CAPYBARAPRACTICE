<!DOCTYPE html>
<html>
  <head>
    <title>GoalApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= flash[:errors] %>
    <%= current_user.username if current_user %>
    <%= button_to "Log OUT", sessions_url, method: "delete"  %>
    <%= yield %>
  </body>
</html>
